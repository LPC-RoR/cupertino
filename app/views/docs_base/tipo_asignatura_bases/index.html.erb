<p id="notice"><%= notice %></p>

<h1>Tipo Asignatura Bases</h1>

<table>
  <thead>
    <tr>
      <th>Orden</th>
      <th>Tipo asignatura</th>
      <th>Curriculum base</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_asignatura_bases.each do |tipo_asignatura_base| %>
      <tr>
        <td><%= tipo_asignatura_base.orden %></td>
        <td><%= tipo_asignatura_base.tipo_asignatura %></td>
        <td><%= tipo_asignatura_base.curriculum_base_id %></td>
        <td><%= link_to 'Show', tipo_asignatura_base %></td>
        <td><%= link_to 'Edit', edit_tipo_asignatura_base_path(tipo_asignatura_base) %></td>
        <td><%= link_to 'Destroy', tipo_asignatura_base, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Asignatura Base', new_tipo_asignatura_base_path %>
