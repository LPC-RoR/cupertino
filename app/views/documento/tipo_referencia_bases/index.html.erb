<p id="notice"><%= notice %></p>

<h1>Tipo Referencia Bases</h1>

<table>
  <thead>
    <tr>
      <th>Orden</th>
      <th>Tipo referencia base</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_referencia_bases.each do |tipo_referencia_base| %>
      <tr>
        <td><%= tipo_referencia_base.orden %></td>
        <td><%= tipo_referencia_base.tipo_referencia_base %></td>
        <td><%= link_to 'Show', tipo_referencia_base %></td>
        <td><%= link_to 'Edit', edit_tipo_referencia_base_path(tipo_referencia_base) %></td>
        <td><%= link_to 'Destroy', tipo_referencia_base, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Referencia Base', new_tipo_referencia_base_path %>
